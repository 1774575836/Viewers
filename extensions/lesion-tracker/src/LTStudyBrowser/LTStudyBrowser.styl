.LTStudyBrowser
  padding: 0 10px;

  .tabButtons
    display: flex
    justify-content: center
    padding: 20px 0
    border-bottom: 1px solid #44626f

  .studyItemBox
    background-color: transparent
    border: 1px solid transparent
    transition: all 0.3s ease

    &:hover
      background-color: #14191e;
      border-color: var(--ui-border-color-dark)

  .roundedButton span
    font-size: 11px
