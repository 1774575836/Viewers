  "repository": "OHIF/Viewers",
  "main": "dist/index.umd.js",
  "module": "src/index.js",
  "sideEffects": "false",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "yarn run dev",
    "dev": "jest --watch",
